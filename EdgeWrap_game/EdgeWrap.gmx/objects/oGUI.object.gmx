            <string>display_set_gui_size(540, 960);
var HUD_OFFSET_X;
if global.LivesEnabled 
    HUD_OFFSET_X = HUD_OFFSET_X_HARDCORE;
else
    HUD_OFFSET_X = HUD_OFFSET_X_REGULAR;

if instance_exists(oChar) {
    // Life counter
    if global.LivesEnabled {
        if global.Lives &gt;= 1
            draw_life_indicator(-1, false);
        else
            draw_life_indicator(-1, true);
        
        if global.Lives &gt;= 2
            draw_life_indicator(0, false);
        else
            draw_life_indicator(0, true);
            
        if global.Lives &gt;= 3
            draw_life_indicator(1, false);
        else
            draw_life_indicator(1, true);
    }
        
    steps += 1;
    if keyboard_check(vk_down) 
        steps += 10;
        
    if steps &gt;= room_speed {
        steps = 0;
        seconds += 1;
        
        if seconds &gt;= 60 {
            seconds = 0;
            minutes += 1;
        }
    }
    
    draw_set_font(FNT_TIMER);
    draw_set_colour(c_black);
    draw_set_alpha(1);
    draw_set_halign(fa_center);
    draw_set_valign(fa_top);
    
    draw_text(GUI_WIDTH-HUD_OFFSET_X, TIMER_OFFSET_Y+HUD_Y, 
              string(minutes)+":" + string_replace_all(string_format(seconds, 2, 0), ' ', '0'));
    
    // Labels
    draw_set_font(FNT_LABELS);
    draw_set_colour(c_black);
    draw_set_alpha(1);
    draw_set_halign(fa_center);
    draw_set_valign(fa_top);
    draw_text(GUI_WIDTH-HUD_OFFSET_X, LABEL_OFFSET_Y, "Timer");
    
    if global.LivesEnabled {
        draw_text(GUI_WIDTH/2, LABEL_OFFSET_Y, "Lives: "+string(global.Lives)+"/3");
    } 
    
    draw_text(HUD_OFFSET_X, LABEL_OFFSET_Y, "Level");
    draw_set_font(FNT_TIMER);
    draw_text(HUD_OFFSET_X, TIMER_OFFSET_Y+HUD_Y, 
              string(global.World) + "-" + string_replace_all(string_format((room + (-10 * global.World + 10)), 2, 0), ' ', '0'))
} else 
    Counting = false;

</string>
